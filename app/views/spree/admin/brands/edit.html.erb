<% content_for :page_title do %>
  <%= link_to Spree.t(:brands), admin_brands_path %> /
  <%= @brand.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @brand } %>

<%= form_for [:admin, @brand], method: :put, html: { multipart: true } do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>